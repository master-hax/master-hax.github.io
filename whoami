<html>
    <script language="Javascript">
      function replaceContent(newContent) {
        document.open();
        document.write(newContent);
        document.close();
      }
      window.fetch("https://vivek.revankar.net/whoami")
        .then(response => response.text())
        .then(data => {
            replaceContent(data);
        }).catch(err => {
            console.log("request failed with error " + err);
            replaceContent("")
        });
    </script>
</html>
